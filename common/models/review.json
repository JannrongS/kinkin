{
  "name": "Review",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "message": {
      "type": "string"
    },
    "rating": {
      "type": "number"
    },
    "create_at": {
      "type": "date",
      "default": "$now"
    },
    "update_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "menu": {
      "type": "belongsTo",
      "model": "Menu",
      "foreignKey": "menuId"
    }
  },
  "acls": [],
  "methods": {}
}
