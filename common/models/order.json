{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string"
    },
    "amount": {
      "type": "number"
    },
    "price": {
      "type": "number"
    },
    "create_at": {
      "type": "date",
      "default": "$now"
    },
    "update_at": {
      "type": "date"
    },
    "userId":{
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
