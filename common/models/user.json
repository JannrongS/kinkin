{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "mobile": {
      "type": "number"
    },
    "picture": {
      "type": "object"
    },
    "isVerify": {
      "type": "boolean"
    },
    "facebookId": {
      "type": "string"
    },
    "device_token": {
      "type": "string"
    },
    "otp_code": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "serviceAccessTokens": {
      "type": "hasMany",
      "model": "ServiceAccessToken",
      "foreignKey": "userId"
    },
    "profile": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "profileId"
    },
    "shop": {
      "type": "belongsTo",
      "model": "Shop",
      "foreignKey": "shopId"
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
