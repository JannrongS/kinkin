{
  "name": "Shop",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "shop_pic": {
      "type": "string"
    },
    "id_card": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "location": {
      "type": "geopoint"
    },
    "bookbank": {
      "type": "string"
    },
    "create_at": {
      "type": "date",
      "default": "$now"
    },
    "update_at": {
      "type": "date"
    },
    "userId":{
      "type": "string"
    },
    "address": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "province": {
      "type": "belongsTo",
      "model": "Province",
      "foreignKey": "provinceId"
    },
    "amphur": {
      "type": "belongsTo",
      "model": "Amphur",
      "foreignKey": "amphurId"
    },
    "district": {
      "type": "belongsTo",
      "model": "District",
      "foreignKey": "districtId"
    }
  },
  "acls": [],
  "methods": {}
}
